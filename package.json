{
  "name": "ca3-cinema",
  "version": "0.1.0",
  "private": true,
  "scripts": {
    "start": "concurrently 'vercel dev' 'nodemon --watch src --watch public --ext ts,tsx --exec yarn build:dev'",
    "build:dev": "esbuild --bundle src/App.tsx --outdir=build/static --loader:.svg=file --loader:.js=jsx --sourcemap && cp -r public/* build && cp -r src/assets build",
    "build": "esbuild --bundle src/App.tsx --outdir=build/static --loader:.svg=file --loader:.js=jsx --minify && cp -r public/* build && cp -r src/assets build",
    "deploy": "yarn build && vercel deploy"
  },
  "dependencies": {
    "@emotion/react": "^11.9.3",
    "@emotion/styled": "^11.9.3",
    "@mui/icons-material": "^5.8.4",
    "@mui/lab": "^5.0.0-alpha.92",
    "@mui/material": "^5.9.2",
    "@types/node": "^18.6.2",
    "@vercel/node": "^2.5.0",
    "axios": "^0.27.2",
    "concurrently": "^7.3.0",
    "cookie": "^0.5.0",
    "esbuild": "^0.14.51",
    "faunadb": "^4.6.0",
    "koa": "^2.13.4",
    "koa-static": "^5.0.0",
    "koa2-history-api-fallback": "^0.1.0",
    "next": "^12.2.3",
    "notistack": "^2.0.5",
    "pleasejs": "^0.4.2",
    "query-string": "^7.1.1",
    "react": "^18.2.0",
    "react-dom": "^18.2.0",
    "react-is": "^18.2.0",
    "react-tiny-popover": "^7.1.0",
    "throttle-debounce": "^5.0.0",
    "ts-node": "^10.9.1",
    "typescript": "^4.7.4",
    "wouter": "^2.8.0-alpha.2"
  },
  "devDependencies": {
    "@types/cookie": "^0.5.1",
    "@types/react": "^18.0.15",
    "@types/react-dom": "^18.0.6",
    "@types/throttle-debounce": "^5.0.0",
    "nodemon": "^2.0.19"
  },
  "packageManager": "yarn@3.2.1"
}
