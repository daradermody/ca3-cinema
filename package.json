{
  "name": "ca3-cinema",
  "version": "0.1.0",
  "private": true,
  "scripts": {
    "start": "concurrently --kill-others 'vercel dev' 'nodemon --watch src --watch public --ext ts,tsx --exec yarn build:dev'",
    "build:dev": "esbuild --bundle src/App.tsx --outdir=build/static --loader:.svg=file --loader:.js=jsx --sourcemap && cp -r public/* build && cp -r src/assets build",
    "build": "esbuild --bundle src/App.tsx --outdir=build/static --loader:.svg=file --loader:.js=jsx --minify && cp -r public/* build && cp -r src/assets build",
    "deploy": "yarn build && vercel deploy",
    "vote-stats": "ts-node scripts/listUnpopularMoves.ts"
  },
  "dependencies": {
    "@emotion/react": "^11.10.5",
    "@emotion/styled": "^11.10.5",
    "@mui/icons-material": "^5.11.0",
    "@mui/lab": "^5.0.0-alpha.112",
    "@mui/material": "^5.11.0",
    "@types/node": "^18.11.16",
    "@vercel/node": "^2.8.1",
    "axios": "^1.2.1",
    "concurrently": "^7.6.0",
    "cookie": "^0.5.0",
    "dotenv": "^16.0.3",
    "esbuild": "^0.16.8",
    "faunadb": "^4.7.1",
    "koa": "^2.14.1",
    "koa-static": "^5.0.0",
    "koa2-history-api-fallback": "^0.1.0",
    "luxon": "^3.1.1",
    "next": "^13.0.7",
    "notistack": "^2.0.8",
    "pleasejs": "^0.4.2",
    "query-string": "^8.0.3",
    "react": "^18.2.0",
    "react-dom": "^18.2.0",
    "react-is": "^18.2.0",
    "react-tiny-popover": "^7.2.0",
    "throttle-debounce": "^5.0.0",
    "ts-node": "^10.9.1",
    "typescript": "^4.9.4",
    "wouter": "^2.9.0"
  },
  "devDependencies": {
    "@types/cookie": "^0.5.1",
    "@types/luxon": "^3.1.0",
    "@types/react": "^18.0.26",
    "@types/react-dom": "^18.0.9",
    "@types/throttle-debounce": "^5.0.0",
    "nodemon": "^2.0.20"
  },
  "packageManager": "yarn@3.2.1"
}
